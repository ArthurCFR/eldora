{
  "expo": {
    "name": "Voyaltis Assistant",
    "slug": "voyaltis-pharmacy-assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.voyaltis.pharmacyassistant"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3B82F6"
      },
      "package": "com.voyaltis.pharmacyassistant"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Cette application a besoin d'acc√©der au microphone pour enregistrer vos rapports de visite."
        }
      ],
      "expo-router"
    ],
    "scheme": "voyaltis",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
